{
	"name": "@ctx-core/up__apex",
	"version": "2.1.6",
	"description": "scripts to manage projects using up (https://up.docs.apex.sh/) on aws lambda or similar services",
	"keywords": [
		"ctx-core",
		"up",
		"aws lambda"
	],
	"homepage": "https://github.com/ctx-core/up__apex#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/up__apex/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/up__apex.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.3.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module",
	"sideEffects": false
}